   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x=[1,2,3,4,5,6,7]\n",
    "y=[3,2,1,4,5,4,7]\n",
    "\n",
    "plt.step(x, y, color=\"r\", marker=\"*\", ms=20, mfc=\"g\", label=\"Python\")\n",
    "\n",
    "plt.grid()\n",
    "plt.title(\"Matplotlib\")\n",
    "plt.xlabel(\"x-axis\")\n",
    "plt.ylabel(\"y-axis\")\n",
    "\n",
    "plt.legend()\n",
    "\n",
    "plt.show()"
   ]
